<!DOCTYPE html>
<html>
<head>
  <title>ReciPipeApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render "nav.html.erb" %>
<div class='flash'>
  <p>
    <%= flash.now[:alert] %>
  </p> 
  <p>
    <%= flash.now[:notice] %>
  </p>
</div>

<%= yield %>

</body>
</html> 
