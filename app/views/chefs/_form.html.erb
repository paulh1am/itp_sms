<%= form_for @chef do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :phone_number %>
<%= f.text_field :phone_number %><br/>
<%= f.label :password %>
<%= f.password_field :password %>
<%= f.label :confirm %>
<%= f.password_field :password_confirmation %><br/>
<%= f.submit %>

<% end %>